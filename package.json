{
  "name": "Sillot-workspace",
  "version": "0.34.1200",
  "description": "汐洛 pnpm 工作区",
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "cli": "node ./package.cli.mjs",
    "app:b:lint": "pnpm --filter ./app run b:lint",
    "app:gentpl": "pnpm --filter ./app run gentpl",
    "app:git:tag": "pnpm --filter ./app run git:tag",
    "siyuan:b:lint": "",
    "dep:check": "pnpm outdated --filter *",
    "dev:docs": "pnpm --filter ./docs/starlight run dev",
    "vsce:sillot": "pnpm --filter ./.vscode/extensions/Sillot run vsce:package",
    "git:proxy": "git config --global http.proxy http://127.0.0.1:7890 && git config --global https.proxy http://127.0.0.1:7890 && echo '7890是 clash for windows 默认端口'",
    "test": "pnpm -r run test"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^4.7.4",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "@biomejs/biome": "1.7.1",
    "shelljs": "^0.8.5",
    "iconv-lite": "^0.6.3",
    "inquirer": "^9.2.20",
    "ora": "^8.0.1"
  }
}